<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date [%thread] %level %logger{35} - %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="org.dcc" level="DEBUG" />
  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration> 
